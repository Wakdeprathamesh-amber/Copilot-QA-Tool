# Database Configuration (Redshift)
DB_HOST=your-redshift-host.com
DB_PORT=5439
DB_NAME=your_database
DB_USER=your_username
DB_PASSWORD=your_password
DB_SSL=false

# Server Configuration
PORT=5000
NODE_ENV=development

# Frontend URL (Required for CORS)
FRONTEND_URL=http://localhost:3000

# JWT Configuration
JWT_SECRET=your-secret-key-here
JWT_EXPIRES_IN=8h

# Redis Configuration (optional)
REDIS_HOST=localhost
REDIS_PORT=6379

# LangSmith Configuration (optional)
LANGSMITH_ORG=your-langsmith-org-id
LANGSMITH_PROJECT=your-langsmith-project-id
# Time range for trace link (trace must fall within this: 7d, 30d, 90d)
LANGSMITH_DURATION=90d
