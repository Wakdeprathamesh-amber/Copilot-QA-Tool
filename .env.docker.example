# Local Docker Environment Variables Template
# Copy this file to .env.docker and fill in your actual values

# Local PostgreSQL (for development)
DB_NAME=ai_eval_console
DB_USER=postgres
DB_PASSWORD=your_postgres_password

# JWT Secret
JWT_SECRET=your-jwt-secret-key

# Redis Password
REDIS_PASSWORD=your_redis_password

# Production Redshift connection (replace with your credentials)
REDSHIFT_HOST=your-redshift-host.com
REDSHIFT_PORT=5439
REDSHIFT_DATABASE=your_database
REDSHIFT_USER=your_username
REDSHIFT_PASSWORD=your_password